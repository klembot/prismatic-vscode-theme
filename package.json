{
	"name": "prismatic",
	"displayName": "Prismatic",
	"description": "A theme for Visual Studio Code that takes Prism.js's default color scheme as direct inspiration",
	"version": "1.8.1",
	"license": "MIT",
	"publisher": "cklimas",
	"repository": {
		"type": "git",
		"url": "https://github.com/klembot/prismatic-vscode-theme.git"
	},
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Themes"
	],
	"contributes": {
		"themes": [
			{
				"label": "Prismatic Light",
				"uiTheme": "vs",
