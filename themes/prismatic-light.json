  "tokenColors": [
    {
      "name": "Attribute Names",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#bf3f59"
      }
    },
    {
      "name": "Attribute Values, Constants, Strings",
      "scope": ["constant", "string.quoted.double.html", "string"],
      "settings": {
        "foreground": "#690"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#708090"
      }
    },
    {
      "name": "JSON Properties, Tag Names",
      "scope": [
        "entity.name.tag",
        "property",
        "support.type.property-name.json",
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#905"
      }
    },
    {
      "name": "Class Names, Functions",
      "scope": ["entity.name.function", "entity.name.type.class"],
      "settings": {
        "foreground": "#905"
      }
    },
    {
      "name": "Keywords, Headings",
      "scope": [
        "entity.name.section.markdown",
        "keyword",
        "keyword.operator.new",
        "storage.modifier",
        "storage.type",
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#07a"
      }
    },
    {
      "name": "Markup Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#c49863"
      }
    },
    {
      "name": "Markup Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#990624"
      }
    },
    {
      "name": "Markup Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#690"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#c49863"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["meta.brace.round", "punctuation"],
      "settings": {
        "foreground": "#999"
      }
    },
    {
      "name": "Resets",
      "scope": ["constant.numeric.css", "keyword.other.unit", "variable"],
      "settings": {
        "foreground": "#000"
      }
    },
    {
      "name": "Selectors, Tags",
      "scope": [
        "entity.name.tag.css",
        "entity.name.tag.html",
        "entity.name.tag.wildcard.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.id.css",
        "punctuation.definition.entity.css",
        "tag"
      ],
      "settings": {
        "foreground": "#07a"
      }
    },
    {
      "name": "Tag Attributes",
      "scope": ["entity.other.attribute-name.html"],
      "settings": {
        "foreground": "#38aba5"
      }
    }
  ]
}
